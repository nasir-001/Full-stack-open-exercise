{
    "plugins": ["react", "jest"],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:jest/recomended"],
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "rules": {
      "react/prop-types": "off",
      "semi": "error"
    }
}